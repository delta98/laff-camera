{
    "name": "laff",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "test": "jest",
        "tsc": "tsc",
        "clean": "rimraf build",
        "build": "yarn  clean && yarn  tsc",
        "lint": "tslint src/**/*.ts",
        "watch": "yarn build -w",
        "ios": "yarn build && concurrently -r \"yarn  watch\" \"react-native run-ios\"",
        "android": "yarn  build && concurrently -r \"yarn  watch\" \"react-native run-android\""
    },
    "dependencies": {
        "react": "16.3.1",
        "react-native": "^0.55.2",
        "react-native-camera": "^1.0.3",
        "react-native-svg": "^6.3.1",
        "react-native-vector-icons": "^4.6.0"
    },
    "devDependencies": {
        "@types/jest": "^22.2.3",
        "@types/react-native": "^0.55.2",
        "@types/react-native-vector-icons": "^4.6.0",
        "babel-jest": "22.4.3",
        "babel-preset-react-native": "4.0.0",
        "concurrently": "^3.5.1",
        "jest": "22.4.3",
        "react-native-cli": "^2.0.1",
        "react-test-renderer": "16.3.1",
        "rimraf": "^2.6.2",
        "tslint": "^5.9.1",
        "typescript": "2.7.1"
    },
    "jest": {
        "preset": "react-native"
    }
}
